<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Milestone_Progress__c</fullName>
    <description>Tracks the Milestone Progress</description>
    <formula>IF(Total_Catchup_Metrics__c = 0, (Total_Completed_Metrics__c / Total_Number_Of_Metrics__c), 1)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Milestone Progress</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
